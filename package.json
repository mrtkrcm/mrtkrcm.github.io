{
  "name": "mrtkrcm",
  "version": "0.0.1",
  "description": "personal experiments and web development stuff",
  "keywords": [],
  "homepage": "https://github.com/mrtkrcm",
  "engines": {
    "node": ">=12",
    "yarn": ">=1.20"
  },
  "scripts": {
    "build": "gatsby build",
    "start": "yarn dev",
    "serve": "gatsby serve",
    "dev": "gatsby develop",
    "fix": "npm-run-all fix:**",
    "fix:lint": "eslint --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "analyze": "npm-run-all analyze:**",
    "analyze:lint": "eslint --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md,json}'",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "setup": "node ./scripts/setup",
    "validate": "npm-run-all --parallel lint test:coverage build",
    "precommit": "npm run validate"
  },
  "dependencies": {
    "gatsby": "2.18.12",
    "gatsby-plugin-manifest": "2.2.33",
    "gatsby-plugin-offline": "3.0.29",
    "gatsby-plugin-postcss": "2.1.18",
    "gatsby-plugin-purgecss": "4.0.1",
    "gatsby-plugin-react-helmet": "3.1.18",
    "gatsby-plugin-sharp": "2.3.8",
    "gatsby-source-filesystem": "2.1.42",
    "gatsby-transformer-sharp": "2.3.9",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "@types/react": "16.9.16",
    "autoprefixer": "9.7.3",
    "babel-eslint": "10.0.3",
    "babel-plugin-import": "1.13.0",
    "cssnano": "4.1.10",
    "eslint": "6.7.2",
    "eslint-plugin-react": "7.17.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "prettier-eslint": "9.0.1",
    "prettier-eslint-cli": "5.0.0",
    "tailwindcss": "1.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {}
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
